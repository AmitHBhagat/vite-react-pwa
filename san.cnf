[ req ]
default_bits       = 2048
prompt            = no
default_md        = sha256
req_extensions    = req_ext
distinguished_name = dn

[ dn ]
C  = IN
ST = Maharashtra
L  = Navi Mumbai
O  = PI Pvt Ltd
CN = 192.168.83.208  # Common Name (CN) - Must match your IP

[ req_ext ]
subjectAltName = PI

[ alt_names ]
IP.1 = 192.168.83.208  # Add your IP address here
